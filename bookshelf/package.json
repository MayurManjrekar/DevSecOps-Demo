{
  "type": "module",
  "name": "nodejs-getting-started",
  "description": "End to end sample for running Node.js applications on Google Cloud Platform",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "engines": {
    "node": ">=12"
  },
  "repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "jest --coverage",
    "test:mocha": "mocha --timeout=8000 --exit **/*.test.js",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@google-cloud/firestore": "^5.0.0",
    "@google-cloud/storage": "^6.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "multer": "^2.0.0",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@eslint/js": "^8.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.0.0",
    "eslint-plugin-security": "^2.0.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^9.0.0",
    "supertest": "^6.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./build/reports/tests",
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "books/**/*.js",
      "lib/**/*.js",
      "!**/node_modules/**"
    ]
  }
}

